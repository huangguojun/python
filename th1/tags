Meal	meal.py	/^class Meal:$/;"	c	inherits:
__init__	meal.py	/^	def __init__(self,food="omelet",drink="coffee"):$/;"	m	class:Meal	access:public
in_fridge	ch5.py	/^def in_fridge():$/;"	f	access:public
m	main.py	/^m = meal.Meal()$/;"	v
printIt	meal.py	/^	def printIt(self,prefix=''):$/;"	m	class:Meal	access:public
setDrink	meal.py	/^	def setDrink(self,drink):$/;"	m	class:Meal	access:public
setFood	meal.py	/^	def setFood(self,food):$/;"	m	class:Meal	access:public
setName	meal.py	/^	def setName(self,name):$/;"	m	class:Meal	access:public
